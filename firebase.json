{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/rooms",
        "destination": "/rooms.html"
      },
      {
        "source": "/dashboard",
        "destination": "/dashboard.html"
      }
    ]
  }
}
